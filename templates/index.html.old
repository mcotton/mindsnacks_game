{% extends "template.html" %}

{% block container %}

<div id='wrapper'>
    <h4></h4>
        <div id='container'>
            <table id='game'>
                <tr>
                    <td id='td_1'><div id='one' class='numbers'>1</div></td>
                    <td id='td_2'><div id='two' class='numbers'>2</div></td>
                    <td id='td_3'><div id='three' class='numbers'>3</div></td>
                </tr>
                <tr>
                    <td id='td_4'><div id='four' class='numbers'>4</div></td>
